{
  "manifest_version": 3,
  "name": "Supabase React Extension",
  "version": "1.0.0",
  "description": "A Chrome Extension built with React, Tailwind, and Supabase.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "favicon.ico",
      "48": "favicon.ico",
      "128": "favicon.ico"
    }
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "host_permissions": [
    "https://*/*"
  ]
}